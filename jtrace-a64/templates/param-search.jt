                                 </action>
                              </instruction>
                              <instruction>
                                 <action>
                                    <set-k-value>
                                       <exponent>20</exponent>
                                    </set-k-value>
                                 </action>
                              </instruction>
                              <instruction>
                                 <action>
                                    <set-alignment>
                                       <alignment>4</alignment>
                                    </set-alignment>
                                 </action>
                              </instruction>
                              <instruction>
                                 <action>
                                    <write-to-a-file>
                                       <file>
                                             <absolute-path>/home/tedstrauss/fdbk</absolute-path>
                                          <name>param-search-0315.txt</name>
                                       </file>
                                       <content>
                                          <arg>
                                             <query>
                                                   <fetch-current-value-of-a-parameter>
                                                      <parameter-name>gamma.w</parameter-name>
                                                   </fetch-current-value-of-a-parameter>
                                             </query>
                                          </arg>
                                          <arg>
                                             <query>
                                                   <fetch-current-value-of-a-parameter>
                                                      <parameter-name>bias</parameter-name>
                                                   </fetch-current-value-of-a-parameter>
                                             </query>
                                          </arg>
                                          <arg>
                                             <query>
                                                   <fetch-current-value-of-a-parameter>
                                                      <parameter-name>alpha.wp</parameter-name>
                                                   </fetch-current-value-of-a-parameter>
                                             </query>
                                          </arg>
                                          <arg>
                                             <query>
                                                   <decision-rule-report>
                                                      <return-type>text</return-type>
                                                      <threshold>0.9</threshold>
                                                      <item>
                                                         <query>
                                                               <current-input>
                                                                  <return-type>text</return-type>
                                                               </current-input>
                                                         </query>
                                                      </item>
                                                      <verbosity>1</verbosity>
                                                   </decision-rule-report>
                                             </query>
                                          </arg>
                                       </content>
                                    </write-to-a-file>
