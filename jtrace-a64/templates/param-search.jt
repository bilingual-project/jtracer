                                       <target-parameter>modelInput</target-parameter>
                                    </use-the-current-lexicon>
                                 </over-items-in-a-lexicon>
                              <instruction>
                                 <action>
                                    <set-cycles-per-sim>
                                       <cycles>100</cycles>
                                    </set-cycles-per-sim>
                                 </action>
                              </instruction>
                              <instruction>
                                 <action>
                                    <add-silence-to-input-edges>
                                       <arg>n/a</arg>
                                    </add-silence-to-input-edges>
                                 </action>
                              </instruction>
                              <instruction>
                                 <action>
                                    <set-graph-domain>
                                       <domain>WORDS</domain>
                                    </set-graph-domain>
                                 </action>
                              </instruction>
                              <instruction>
                                 <action>
                                    <set-watch-type>
                                       <type>WATCH-TOP-N</type>
                                    </set-watch-type>
                                 </action>
                              </instruction>
                              <instruction>
                                 <action>
                                    <set-watch-top-n>
                                       <N>10</N>
                                    </set-watch-top-n>
                                 </action>
                              </instruction>
                              <instruction>
                                 <action>
                                    <set-analysis-type>
                                       <type>SPECIFIED</type>
                                    </set-analysis-type>
                                 </action>
                              </instruction>
                              <instruction>
                                 <action>
                                    <set-choice-type>
                                       <type>NORMAL</type>
                                    </set-choice-type>
                                 </action>
                              </instruction>
                              <instruction>
                                 <action>
                                    <set-content-type>
